{
  "name": "docbare",
  "version": "0.1.0",
  "private": true,
  "description": "DocBare: Modern legal AI platform for document drafting and analysis",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed-firestore": "ts-node scripts/seed-firestore.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:backup": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d_%H%M%S).sql",
    "db:reset": "prisma migrate reset --force",
    "db:migrate": "prisma migrate dev --create-only",
    "db:deploy": "prisma migrate deploy",
    "safe-migration": "ts-node --project tsconfig.scripts.json scripts/safe-migration.ts",
    "test:memory": "ts-node --project tsconfig.scripts.json scripts/test-memory-system.ts",
    "test:migration": "ts-node --project tsconfig.scripts.json scripts/test-database-migration.ts",
    "test:api": "ts-node --project tsconfig.scripts.json scripts/test-api-endpoints.ts",
    "test:continuity": "ts-node --project tsconfig.scripts.json scripts/test-conversation-continuity.ts",
    "test:all-scripts": "npm run test:memory && npm run test:migration && npm run test:api && npm run test:continuity",
    "test:query-api": "ts-node --project tsconfig.scripts.json scripts/test-query-api.ts",
    "deploy:firestore-indexes": "node scripts/deploy-firestore-indexes.js"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@google-cloud/aiplatform": "^4.3.0",
    "@google-cloud/firestore": "^7.11.2",
    "@google-cloud/storage": "^7.16.0",
    "@google/genai": "^1.8.0",
    "@heroicons/react": "^2.1.1",
    "@langchain/core": "^0.3.61",
    "@langchain/openai": "^0.5.15",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.10.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.0",
    "dotenv": "^17.0.1",
    "framer-motion": "^11.0.3",
    "langchain": "^0.3.29",
    "lucide-react": "^0.525.0",
    "next": "14.1.0",
    "next-auth": "^4.24.11",
    "openai": "^5.9.0",
    "pino": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "postcss": "^8.4.35",
    "prisma": "^6.10.1",
    "supertest": "^6.3.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
